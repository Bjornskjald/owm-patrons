{
  "version": 2,
  "builds": [
    { "src": "new_patron/new_patron.go", "use": "@now/go" },
    { "src": "patrons/patrons.go", "use": "@now/go" },
    { "src": "generate_url/generate_url.go", "use": "@now/go" }
  ],

  "routes": [
    { "src": "/patrons", "dest": "/patrons/patrons.go" },
    { "src": "/new-patron", "dest": "/new_patron/new_patron.go" },
    { "src": "/generate-url", "dest": "/generate_url/generate_url.go" }
  ],
  "env": {
    "GITHUB_API_KEY": "@github-api-key",
    "PRIVATE_KEY": "@private-key"
  },
  "regions": ["bru"],
  "alias": ["owm-patrons.now.sh"]
}
